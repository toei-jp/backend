<div class="form-group ">
    <label for="subject" class="col-sm-3 control-label label-title">
        細目<font color="red">＊</font>
    </label>
    <div class="col-sm-9">
        <select class="form-control" name="subject">
            <option value="" disabled selected>選択してください</option>
            <% for (let subject of subjectList) { %>
            <option value="<%= subject.detailCd %>" <% if (forms.subject==subject.detailCd) { %> selected
                <% } %> >
                <%= subject.detailName %>
            </option>
            <% } %>
        </select>
        <p class="error message text-danger">
            <% if (errors && errors.subject) { %>
